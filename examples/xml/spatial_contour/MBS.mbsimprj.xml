<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MBSimProject xmlns="http://www.mbsim-env.de/MBSimXML" name="Project">

  <Embed xmlns="http://www.mbsim-env.de/MBXMLUtils">
    <Parameter>
      <scalarParameter name="m" unit="-">0.7</scalarParameter>
      <scalarParameter name="a" unit="-">0.4</scalarParameter>
      <scalarParameter name="b" unit="-">0.4</scalarParameter>
      <scalarParameter name="c" unit="-">0.4</scalarParameter>
    </Parameter>
    <DynamicSystemSolver xmlns="http://www.mbsim-env.de/MBSim" name="MBS">
      <frames/>
      <contours/>
      <groups/>
      <objects>
        <RigidBody name="RigidBody1">
          <initialGeneralizedPosition>
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0.01</ele>
              <ele>0.45</ele>
              <ele>0.01</ele>
            </xmlVector>
          </initialGeneralizedPosition>
          <mass unit="kg">m</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <stateDependentTranslation>
            <TranslationAlongAxesXYZ/>
          </stateDependentTranslation>
          <frames/>
          <contours>
            <Point name="Point1">
              <enableOpenMBV>
                <?OPENMBV_ID 8?>
                <size unit="m">0.01</size>
              </enableOpenMBV>
            </Point>
          </contours>
          <enableOpenMBVFrameC>
            <?OPENMBV_ID 5?>
            <size unit="cm">1</size>
          </enableOpenMBVFrameC>
        </RigidBody>
        <RigidBody name="RigidBody2">
          <mass unit="kg">1</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <frames/>
          <contours>
            <SpatialContour name="SpatialContour3">
              <nodes unit="-">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </nodes>
              <contourFunction>
                <ContinuedFunction>
                  <function>
                    <SymbolicFunction arg1="zeta" arg1Dim="2">
                      <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                        <ele>a*cos(zeta(1))*cos(zeta(2))</ele>
                        <ele>b*sin(zeta(1))*cos(zeta(2))</ele>
                        <ele>c*sin(zeta(2))</ele>
                      </xmlVector>
                    </SymbolicFunction>
                  </function>
                  <continuationRule>
                    <SymbolicFunction arg1="x" arg1Dim="2">     n = floor((x(2)+pi/2)/pi);
 eta = x(1) + n*pi;
 s = (-1)^abs(n);
t = s*x(2)+pi/2;
xi = (t - pi * floor (t / pi))-pi/2;
ret = [eta;xi]</SymbolicFunction>
                  </continuationRule>
                </ContinuedFunction>
              </contourFunction>
              <enableOpenMBV>
                <?OPENMBV_ID 15?>
              </enableOpenMBV>
            </SpatialContour>
          </contours>
          <openMBVRigidBody>
            <Sphere xmlns="http://www.mbsim-env.de/OpenMBV" name="Body1">
              <?OPENMBV_ID 11?>
              <transparency unit="-">0.3</transparency>
              <initialTranslation unit="m">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </initialTranslation>
              <initialRotation unit="rad">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </initialRotation>
              <scaleFactor unit="-">1</scaleFactor>
              <radius unit="m">a</radius>
            </Sphere>
          </openMBVRigidBody>
          <enableOpenMBVFrameC>
            <?OPENMBV_ID 12?>
            <size unit="cm">1</size>
          </enableOpenMBVFrameC>
        </RigidBody>
      </objects>
      <links>
        <Contact name="Contact1">
          <normalForceLaw>
            <UnilateralConstraint/>
          </normalForceLaw>
          <normalImpactLaw>
            <UnilateralNewtonImpact>
              <restitutionCoefficient unit="-">0</restitutionCoefficient>
            </UnilateralNewtonImpact>
          </normalImpactLaw>
          <connect ref1="../Object[RigidBody2]/Contour[SpatialContour3]" ref2="../Object[RigidBody1]/Contour[Point1]"/>
          <initialGuess>
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>pi/2</ele>
              <ele>0</ele>
            </xmlVector>
          </initialGuess>
          <enableOpenMBVContactPoints>
            <?OPENMBV_ID 22?>
            <size unit="cm">1</size>
          </enableOpenMBVContactPoints>
        </Contact>
      </links>
      <enableOpenMBVFrameI>
        <?OPENMBV_ID 3?>
        <size unit="cm">1</size>
      </enableOpenMBVFrameI>
      <environments>
        <MBSimEnvironment>
          <accelerationOfGravity unit="m/s^2">
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>-9.81</ele>
              <ele>0</ele>
            </xmlVector>
          </accelerationOfGravity>
        </MBSimEnvironment>
      </environments>
    </DynamicSystemSolver>
  </Embed>

  <LSODARIntegrator xmlns="http://www.mbsim-env.de/MBSimIntegrator">
    <startTime unit="s">0</startTime>
    <endTime unit="s">1.5</endTime>
    <plotStepSize unit="s">1e-2</plotStepSize>
    <absoluteToleranceScalar>1e-6</absoluteToleranceScalar>
    <relativeToleranceScalar unit="-">1e-6</relativeToleranceScalar>
    <initialStepSize unit="s">1e-8</initialStepSize>
    <minimalStepSize unit="s">0</minimalStepSize>
    <maximalStepSize unit="s">0</maximalStepSize>
    <plotOnRoot>0</plotOnRoot>
  </LSODARIntegrator>

</MBSimProject>
